<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.LoginController">
    <children>
        <Label layoutX="180.0" layoutY="53.0" text="Login" />
        <Label layoutX="64.0" layoutY="97.0" text="Username" />
        <TextField fx:id="usernameField" layoutX="134.0" layoutY="93.0" />
        <Label layoutX="67.0" layoutY="129.0" text="Password" />
        <PasswordField fx:id="passwordField" layoutX="134.0" layoutY="125.0" />
        <Button fx:id="btnLogin" layoutX="172.0" layoutY="167.0" mnemonicParsing="false" text="Login"  onAction="#handleLogin" />
        <Label fx:id="messageLabel" layoutX="83.0" layoutY="28.0" prefHeight="17.0" prefWidth="234.0" />
    </children>
</AnchorPane>
